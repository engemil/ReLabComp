{
	"name": "labscripts-dev-env",
    "dockerComposeFile": "docker-compose.yml",
    "service": "labcontainer",
	"workspaceFolder": "/home/labuser/workspace",
    "remoteUser": "labuser",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-vscode.ccptools",
                "twxs.cmake"
            ]
        }
    }
}